{
  "get": {
    "description": "Returns the subaccount corresponding to the accountSid and subaccountSid provided",
    "responses": {
      "200": {
        "description": "Successfully returned subaccount",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../schemas/Subaccount.json"
            }
          }
        }
      },
      "default": {
        "description": "Failed to fetch subaccount.",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../schemas/ErrorModel.json"
            }
          }
        }
      }
    },
    "parameters": [
      {
        "name": "accountSid",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string"
        },
        "style": "simple"
      },
      {
        "name": "subaccountSid",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string"
        },
        "style": "simple"
      }
    ]
  }
}